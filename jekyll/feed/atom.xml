---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="https://www.w3.org/2005/Atom">

	<title>ElasticDog.com</title>
	<link href="https://elasticdog.com/atom.xml" rel="self" />
	<link href="https://elasticdog.com/" />
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://elasticdog.com/</id>
	<author>
		<name>Aaron Bull Schaefer</name>
		<email>aaron@elasticdog.com</email>
	</author>

	{% for post in site.posts %}
	<entry>
		<title>{{ post.title }}</title>
		<link href="https://elasticdog.com{{ post.url }}" />
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>https://elasticdog.com{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry>
	{% endfor %}

</feed>
